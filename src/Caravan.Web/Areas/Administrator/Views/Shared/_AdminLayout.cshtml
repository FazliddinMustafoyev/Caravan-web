                        <a class="btn btn-secondary" style="width:100%;" asp-area="administrator" asp-controller="accounts" asp-action="register">
                            Sign Up
                        </a>
                    </div>

                    

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

