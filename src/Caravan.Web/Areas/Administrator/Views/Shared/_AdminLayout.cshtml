            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="index.html">
                <div>
                    <img alt="Logo" src="~/media/logos/caravan-svgrepo-com.svg" class="h-55px" />
                </div>
                <div class="sidebar-brand-text mx-3">Caravan Admin Page<sup></sup></div>
            </a>

            <!-- Nav Item - Pages Collapse Menu -->
